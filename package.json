{
  "name": "push-urbanairship",
  "fullName": "Urban Airship Push Notifications",
  "version": "0.0.17",
  "keywords": [
    "push",
    "notifications",
    "urban airship"
  ],
  "description": "Setup a Urban Airship account and start pushing right away!<br>iOS and Android only",
  "joshfire": {
    "devices": [
      "phone-ios",
      "phone-android",
      "tablet-ios",
      "tablet-android"
    ],
    "hooks": [
      "plugman",
      "xcodeproj",
      "androidproj"
    ],
    "options": {
      "schema": {
        "dev-app-key": {
          "type": "string",
          "title": "App Key"
        },
        "dev-app-secret": {
          "type": "string",
          "title": "App Secret"
        },
        "prod-app-key": {
          "type": "string",
          "title": "App Key"
        },
        "prod-app-secret": {
          "type": "string",
          "title": "App Secret"
        },
        "gcmSender": {
          "type": "string",
          "title": "GCM Sender ID",
          "joshfire:definedFor": ["phone-android", "tablet-android"]
        }
      },
      "form": [
        {
          "type": "help",
          "helpvalue": "Confused by all these fields? Keep calm and check our <a href=\"http://docs.joshfire.com/doc/create/addons\">guide</a>"
        },
        "gcmSender",
        {
          "type": "fieldset",
          "title": "Development",
          "items": [
            "dev-app-key",
            "dev-app-secret"
          ]
        },
        {
          "type": "fieldset",
          "title": "Production",
          "items": [
            "prod-app-key",
            "prod-app-secret"
          ]
        }
      ]
    }
  }
}
