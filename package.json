{
  "name": "push-urbanairship",
  "fullName": "Urban Airship Push Notifications",
  "version": "0.0.11",
  "keywords": [
    "push",
    "notifications",
    "urban airship"
  ],
  "description": "Push notifications with Urban Airship",
  "joshfire": {
    "images": {
      "small": "marketing/ua-logo.png",
      "big":"marketing/ua-logo.png"
    },
    "devices": [
      "phone-ios",
      "phone-android",
      "tablet-ios",
      "tablet-android"
    ],
    "hooks": [
      "injectCordovaPluginJS",
      "xcodeproj",
      "androidproj"
    ],
    "options": {
      "schema": {
        "dev-app-key": {
          "type": "string",
          "title": "App Key"
        },
        "dev-app-secret": {
          "type": "string",
          "title": "App Secret"
        },
        "prod-app-key": {
          "type": "string",
          "title": "App Key"
        },
        "prod-app-secret": {
          "type": "string",
          "title": "App Secret"
        },
        "gcmSender": {
          "type": "string",
          "title": "GCM Sender",
          "joshfire:definedFor": ["phone-android", "tablet-android"]
        }
      },
      "form": [
        "gcmSender",
        {
          "type": "fieldset",
          "title": "Development",
          "items": [
            "dev-app-key",
            "dev-app-secret"
          ]
        },
        {
          "type": "fieldset",
          "title": "Production",
          "items": [
            "prod-app-key",
            "prod-app-secret"
          ]
        }
      ]
    }
  }
}
